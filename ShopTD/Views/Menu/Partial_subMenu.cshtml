@using ShopTD.Models
@model List<Menu>

@foreach(var item in Model)
{
    <li class="nav-item">
        <a class="nav-link" href="#">@item.Name</a>
        <ul class="sub_menu nav" style="margin-left: 200px; margin-top: -40px;">
            @Html.Action("Partial_subMenu", "Menu", new { parent_id = item.Menu_ID })
        </ul>
    </li>
}

